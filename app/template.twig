<html class="no-js"> <!--<![endif]-->
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>{{ page['meta']['title'] }}</title>
	<meta name="description" content="">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">

	<!-- Place favicon.ico and apple-touch-icon.png in the root directory -->

	<link rel="stylesheet" href="/app/style.{{ _v }}.css">
	{% if page['template_css'] %}
		<link rel="stylesheet" href="{{ page['template_css'] }}">
	{% endif %}
	<script src="/vendor/components/modernizr/modernizr.{{ _v }}.js"></script>
</head>
<body>
<!--[if lt IE 9]>
<p class="browsehappy">You are using an <strong>outdated</strong> browser. Please
	<a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
<![endif]-->

<!-- Add your site or application content here -->
{% if _user['ID'] %}
{%  include "_header.twig" %}
{% else %}
	{%  include "_header_front.twig" %}
{% endif %}
<section class="container" id="page-content">
	{%  include page['template'] %}
</section>
<div style="position: fixed; left:10px; bottom:10px; z-index:999999999;">
	<a href="#systemTimers-container" data-toggle="modal" data-target="#systemTimers-container"><i class="fa fa-clock-o"></i></a>
</div>
{%  include "_footer.twig" %}
<script src="/app/javascript.{{ _v }}.js"></script>
<script src="/vendor/ckeditor/ckeditor/ckeditor.js?v={{ _v }}"></script>

{% for row in page['js'] %}
	<script src="{{row }}"></script>
{% endfor %}
{% if page['template_js'] %}
<script src="{{ page['template_js'] }}"></script>
{% endif %}

{% if page['template_jtmpl'] %}
{% include page['template_jtmpl'] %}
{% endif %}

{% include 'template/template.jtmpl' %}

<div class="modal fade" id="systemTimers-container" tabindex="-1" role="dialog" aria-labelledby="SystemTimers" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
				<h4 class="modal-title" id="myModalLabel">Debug</h4>
			</div>
			<div class="modal-body">
				<article>
					<table id='systemTimers' class="table table-condensed">

					</table>
				</article>
			</div>
		</div>
	</div>
</div>

<div class="modal fade" id="systemTimers-container" tabindex="-1" role="dialog" aria-labelledby="systemTimers-container" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class='modal-header'><a href='#' class='close' data-dismiss="modal">&times;</a>
				<h3>Debug</h3>
			</div>
			<div class='modal-body'>
				<article>
					<table id='systemTimers' class="table table-condensed">

					</table>
				</article>
			</div>
			<div class='modal-footer'>

			</div>
		</div>
	</div>
</div>

<div class="modal fade" id="form-modal" tabindex="-1" role="dialog" aria-labelledby="form-modal" aria-hidden="true"></div>

